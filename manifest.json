{
  "manifest_version": 2,
  "name": "atoyomi - あとで読む",
  "version": "1.1.0",
  "description": "atoyomiは、簡易的な「あとで読む」機能を追加するChrome Extensionです。",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "あとで読む",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}